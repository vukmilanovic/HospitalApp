        <Button Content="Registruj se" HorizontalAlignment="Left" Margin="231,162,0,0" VerticalAlignment="Top" Click="RegistrationClick"/>
        <TextBox HorizontalAlignment="Left" Name="username" Margin="151,82,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
